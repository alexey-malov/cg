    <RmColorVariable NAME="baseColor" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE_0="0.721569" VALUE_1="0.704711" VALUE_2="0.760274" VALUE_3="1.000000"/>
